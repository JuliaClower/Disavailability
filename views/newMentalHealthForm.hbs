<h1>Create New Mental Health</h1>

<form action="/mentalHealth" method="POST"/>
    <input type="text" name="category" placeholder="category" />
    <input type="text" name="name" placeholder="name" />
    <input type="text" name="depressionHelp" placeholder="depressionHelp" />
    <input type="text" name="anxietyHelp" placeholder="anxietyHelp" />
    <input type="text" name="ptsdHelp" placeholder="ptsdHelp" />
    <input type="text" name="description" placeholder="description" />
    <input type="text" name="picture" placeholder="picture" />

    <input class="allButton" type="submit"    value="Create"/>
</form>
