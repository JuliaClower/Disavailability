<h1>Edit Mental Health</h1>

<form action="/handicap/{{_id}}?_method=PUT" method="POST"/>
    <input type="text" name="category" placeholder="category" value="{{category}}"/>
    <input type="text" name="name" placeholder="name" value="{{name}}"/>
    <input type="text" name="depressionHelp" placeholder="depressionHelp" value="{{depressionHelp}}"/>
    <input type="text" name="anxietyHelp" placeholder="anxietyHelp" value="{{anxietyHelp}}"/>
    <input type="text" name="ptsdHelp" placeholder="ptsdHelp" value="{{ptsdHelp}}"/>
    <input type="text" name="description" placeholder="description" value="{{description}}"/>
    <input type="text" name="picture" placeholder="picture" value="{{picture}}"/>

    <input type="submit"    value="Save"/>
</form>